{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host={HOST};Database={DB};Username={USERNAME};Password={PASSWORD}"
  },
  "RabbitMQ": {
    "ConnectionString": "amqp://{USERNAME}:{PASSWORD}@rabbitmq:5672/",
    "SendEmailQueue": "send-email"
  },
  "AllowedHosts": "*"
}
