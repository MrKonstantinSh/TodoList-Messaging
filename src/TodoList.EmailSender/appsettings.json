{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMQ": {
    "ConnectionString": "amqp://{USERNAME}:{PASSWORD}@rabbitmq:5672/",
    "SendEmailQueue": "send-email"
  },
  "EmailSender": {
    "SmtpServer": "smtp.gmail.com",
    "Port": "587",
    "EmailFrom": "{EMAIL_FROM}",
    "PasswordFrom": "{EMAIL_FROM_PASSWORD}"
  },
  "AllowedHosts": "*"
}
